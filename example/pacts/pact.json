{
  "consumer": {
    "name": "docker-example-consumer"
  },
  "provider": {
    "name": "docker-example-provider"
  },
  "interactions": [
    {
      "description": "a request for bar",
      "providerState": "bar exists",
      "request": {
        "method": "GET",
        "path": "/bar",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "id": "1",
          "name": "Bar"
        }
      }
    }
  ],
  "metdata": {
    "pactSpecification": {
      "version": "2"
    }
  }
}
